<!DOCTYPE html>
<html>
  <head>
    <title><%= t ".title" %></title>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <%= favicon_link_tag 'favicon.ico' %>

  </head>
  <body>

    <%= render "layouts/header" %>
    <%= yield %>
    <%= render "layouts/footer" if render_footer? %>

  </body>
</html>
